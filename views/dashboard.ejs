    <script src="//cdnjs.cloudflare.com/ajax/libs/morris.js/0.5.1/morris.min.js"></script>
    <script>
        Morris.Bar({
            // ID of the element in which to draw the chart.
            element: 'line-chart1',
            // Chart data records -- each entry in this array corresponds to a point on
            // the chart.
            data: [{
                year: '<%= reg[6].day %>',
                value: <%= reg[6].count %>
            }, {
                year: '<%= reg[5].day %>',
                value: <%= reg[5].count %>
            }, {
                year: '<%= reg[4].day %>',
                value: <%= reg[4].count %>
            }, {
                year: '<%= reg[3].day %>',
                value: <%= reg[3].count %>
            }, {
                year: '<%= reg[2].day %>',
                value: <%= reg[2].count %>
            }, {
                year: '<%= reg[1].day %>',
                value: <%= reg[1].count %>
            }, {
                year: '<%= reg[0].day %>',
                value: <%= reg[0].count %>
            }],
            // The name of the data record attribute that contains x-values.
            xkey: 'year',
            // A list of names of data record attributes that contain y-values.
            ykeys: ['value'],
            // Labels for the ykeys -- will be displayed when you hover over the
            // chart.
            labels: ['Value']
        });
        Morris.Bar({
            // ID of the element in which to draw the chart.
            element: 'line-chart',

            data: [{
                year: '<%= sur[6].day %>',
                value: <%= sur[6].count %>
            }, {
                year: '<%= sur[5].day %>',
                value: <%= sur[5].count %>
            }, {
                year: '<%= sur[4].day %>',
                value: <%= sur[4].count %>
            }, {
                year: '<%= sur[3].day %>',
                value: <%= sur[3].count %>
            }, {
                year: '<%= sur[2].day %>',
                value: <%= sur[2].count %>
            }, {
                year: '<%= sur[1].day %>',
                value: <%= sur[1].count %>
            }, {
                year: '<%= sur[0].day %>',
                value: <%= sur[0].count %>
            }],
            // The name of the data record attribute that contains x-values.
            xkey: 'year',
            // A list of names of data record attributes that contain y-values.
            ykeys: ['value'],
            // Labels for the ykeys -- will be displayed when you hover over the
            // chart.
            labels: ['Value']
        });
        Morris.Line({
            // ID of the element in which to draw the chart.
            element: 'revenue-chart',

            data: [{
                year: '<%= payment[6].day %>',
                value: <%= payment[6].count %>
            }, {
                year: '<%= payment[5].day %>',
                value: <%= payment[5].count %>
            }, {
                year: '<%= payment[4].day %>',
                value: <%= payment[4].count %>
            }, {
                year: '<%= payment[3].day %>',
                value: <%= payment[3].count %>
            }, {
                year: '<%= payment[2].day %>',
                value: <%= payment[2].count %>
            }, {
                year: '<%= payment[1].day %>',
                value: <%= payment[1].count %>
            }, {
                year: '<%= payment[0].day %>',
                value: <%= payment[0].count %>
            }],
            // The name of the data record attribute that contains x-values.
            xkey: 'year',
            // A list of names of data record attributes that contain y-values.
            ykeys: ['value'],
            // Labels for the ykeys -- will be displayed when you hover over the
            // chart.
            labels: ['Value']
        });
    </script>



